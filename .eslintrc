{
  "env": {
    "browser": 1,
    "node": 1,
    "es6": 1
  },
  "rules": {
    "quotes": [2, "single"],
    "strict": [2, "never"],
    "no-multi-spaces": 0,
    "no-alert": 0,
    "key-spacing": 0,
    "curly": 0,
    "no-octal-escape": 0,
    "no-use-before-define": 0,
    "new-cap": 0
  },
  "ecmaFeatures": {
    "modules": 1
  }
}